{
    "name": "zicht/framework-extra-bundle",
    "description": "Utilities for Symfony",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.2 || ^8",
        "doctrine/persistence": "1.3 - 3",
        "swaggest/json-schema": "^0.12",
        "symfony/console": "^4.4",
        "symfony/form": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/routing": "^4.4",
        "twig/twig": "^2.7 || ^3",
        "zicht/util": "^2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\FrameworkExtraBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\FrameworkExtraBundle\\": "tests/"
        }
    },
    "license": "MIT",
    "scripts": {
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "test": "phpunit"
    }
}
