{
    "name": "zicht/framework-extra-bundle",
    "description": "Utilities for Symfony",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^8",
        "doctrine/dbal": "^2.13.1 || ^3",
        "doctrine/persistence": "1.3 - 3",
        "swaggest/json-schema": "^0.12",
        "symfony/console": "^5.4 || ^6",
        "symfony/form": "^5.4 || ^6",
        "symfony/http-foundation": "^5.4 || ^6",
        "symfony/routing": "^5.4 || ^6",
        "symfony/security-acl": "^3",
        "symfony/security-core": "^5.4 || ^6",
        "symfony/translation": "^5.4 || ^6",
        "twig/twig": "^2.7 || ^3",
        "zicht/util": "^2"
    },
    "require-dev": {
        "doctrine/cache": "^1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.13",
        "friendsofphp/php-cs-fixer": "^3",
        "gedmo/doctrine-extensions": "^3",
        "phpunit/phpunit": "^9.6",
        "symfony/cache": "^5.4 || ^6",
        "symfony/doctrine-bridge": "^5.4 || ^6",
        "symfony/framework-bundle": "^5.4 || ^6",
        "symfony/twig-bundle": "^5.4 || ^6",
        "symfony/validator": "^5.4 || ^6",
        "vimeo/psalm": "^5"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\FrameworkExtraBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\FrameworkExtraBundle\\": "tests/"
        }
    },
    "license": "MIT",
    "scripts": {
        "analysis": "psalm",
        "lint": "PHP_CS_FIXER_IGNORE_ENV=1 php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "PHP_CS_FIXER_IGNORE_ENV=1 php-cs-fixer fix",
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-feature-symfony-6": "11.x-dev"
        }
    }
}
